    <body> 
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark" style="
    opacity: 0.94;">
        <a class="navbar-brand" href="#">Jaggery Traceability</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarText">
          <ul class="navbar-nav mr-auto">
            <li class="nav-item active">
              <a class="nav-link" href="/dashboard">Home</a>
            </li>
            <li class="nav-item active" style="background-color:lightgray;">
              <a class="nav-link text-dark" href="#"><b>Update Profile</b><span class="sr-only">(current)</span></a>
            </li>
            <%if ( role  == 'Manufacturer') { %>                                     
              <li class="nav-item active">
                <a class="nav-link" href="/allproducts">All Products</a>
              </li>
            <% } %>

            <li class="nav-item active">
              <a class="nav-link" href="/aboutus">About Us</a>
            </li>
          </ul>
          <span class="navbar-text white-text" style="color:white">
            <%= name %>
          </span> &nbsp;&nbsp;&nbsp;
          <a class="btn btn-danger " href="/logout">Logout</a>
        </div>
      </nav>

        <div class="container">
          <div class="jumbotron">
              <h2 class="display-7 text-center"> Update Profile</h2>
                <br/>
                <form  class="form-signin" id="form2" method="post" >
                  <input type="email" id="inputEmail" name="email" value=" <%= email %>" readonly="readonly" class="form-control" placeholder="Email address" required autofocus><br/>
                  <input type="text" name="phoneno" id="Phone-number" value=" <%= phoneno %>" class="form-control" placeholder="Phone Number" required>
                  </br>
                  <input type="text" name="altphoneno" id="Phone-number" placeholder="Alternate Phone no" value=" <%= altphoneno %>" class="form-control" placeholder="Alternate Phone Number" required>
                  </br>
                  <textarea  type="text" name="address"  placeholder="Address" class="form-control" required><%= add %></textarea>
                  
                  <button class="btn btn-lg btn-success btn-block" value="Change" type="submit" id="login-btn" style="margin-left: 0;">Update</button>
                  
                  <span style=" font-weight: bolder;" id="check"></span>
                  
                  <div id="LangTable">
                    <a href="/dashboard" id="aa"></a>
                  </div>
                </form>
          </div>
              
        </div>            
      </body>
    <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
