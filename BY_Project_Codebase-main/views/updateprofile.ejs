    <script>
    VANTA.WAVES({
      el: "#vantajs",
      mouseControls: true,
      touchControls: false,
      gyroControls: false,
      minHeight: 1000.00,
      minWidth: 1000.00,
      scale: 1.00,
      scaleMobile: 1.00,
      shininess: 45.00,
      waveHeight: 10.00,
      waveSpeed: 0.50,
      zoom: 0.7
    })
    </script>
